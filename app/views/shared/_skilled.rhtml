<% rounded(:class => 'blue_gradient user' ) do %>
	<div class="gradient">
		<h2 class="super"><%= link_to "Find an expert", skills_path %></h2>
	</div>
	Get help from someone who knows.
	<ul>
	<% Skill.find(:all, :order => "name").each do |skill| %>
		<li><%= link_to skill.name, skill_path(skill) %></li>
	<% end %>
	</ul>
<% end %>
		
